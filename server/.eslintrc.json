{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": [ "error", 4 ],
        "padded-blocks": "off",
        "no-underscore-dangle": "off",
        "no-console": "off",
        "func-names": "off"
    }
}
